<%- include("partials/header"); -%>

<h1>Compose</h1>

<form class="composeForm" action="/compose" method="post">
  <div class="form-group">
    <label>Text</label>
    <input class="form-control" type="text" name="yourText">
    <label>Post</label>
    <textarea class="form-control" name="yourPost" rows="5" cols="30"></textarea>
  </div>
  <button class="btn btn-primary" type="submit" name="submit">Publish</button>
</form>

